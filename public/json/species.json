  "Goliath": {
    "creature type": "Humanoid",
    "size": "Medium",
    "speed": 35,
    "darkvision": {
      "has darkvision": false
    },
    "languages": [
      { "core languages": "Common, Giant" },
      {
        "additional language": [
          { "language": "Common Sign Language" },
          { "language": "Draconic" },
          { "language": "Dwarvish" },
          { "language": "Elvish" },
          { "language": "Gnomish" },
          { "language": "Goblin" },
          { "language": "Halfling" },
          { "language": "Orc" }
        ]
      }
    ],
    "subspecies": [
      {
        "type": "Cloud",
        "subspecies trait": {
          "name": "Cloud's Jaunt",
          "description": "As a Bonus Action, the Goliath can magically teleport up to 30 feet to an unoccupied space they can see."
        }
      },
      {
        "type": "Fire",
        "subspecies trait": {
          "name": "Fire's Burn",
          "description": "When the Goliath hit a target with an attack roll and deals damage to it, they can also deal 1d10 Fire damage to that target."
        }
      },
      {
        "type": "Frost",
        "subspecies trait": {
          "name": "Cloud's Jaunt",
          "description": "When the Goliath hits a target with an attack roll and deal damage to it, they can also deal 1d6 Cold damage to that target and reduce its Speed by 10 feet until the start of the Goliath's next turn."
        }
      },
      {
        "type": "Hill",
        "subspecies trait": {
          "name": "Hill's Tumble",
          "description": "When the Goliath hits a Large or smaller creature with an attack roll and deal damage to it, they can give that target the Prone condition."
        }
      },
      {
        "type": "Stone",
        "subspecies trait": {
          "name": "Stone's Endurance",
          "description": "When the Goliath takes damage, they can take a Reaction to roll 1d12. Add the Goliath's Constitution modifier to the number rolled and reduce the damage by that total."
        }
      },
      {
        "type": "Storm",
        "subspecies trait": {
          "name": "Storm's Thunder",
          "description": "When the Goliath take damage from a creature within 60 feet of them, they can take a Reaction to deal 1d8 Thunder damage to that creature."
        }
      }
    ],
    "traits": {
      "name": "Powerful Build",
      "description": "The Goliath has Advantage on any ability check they make to end the Grappled condition."
    },
    "bonus action": {
      "name": "Large Form",
      "description": "If they are in a big enough space, the Goliath can change their size to Large as a Bonus Action. This transformation lasts for 10 minutes or until they end it (no action required). For that duration, the Goliath has Advantage on Strength checks, and their Speed increases by 10 feet."
    }
  },

