    "subspecies": [
      { "type": "Black", "element": "Acid" },
      { "type": "Blue", "element": "Lightning" },
      { "type": "Brass", "element": "Fire" },
      { "type": "Bronze", "element": "Lightning" },
      { "type": "Copper", "element": "Acid" },
      { "type": "Gold", "element": "Fire" },
      { "type": "Green", "element": "Poison" },
      { "type": "Red", "element": "Fire" },
      { "type": "Silver", "element": "Cold" },
      { "type": "White", "element": "Cold" }
    ],
    "bonus action": {
      "name": "Draconic Flight",
      "description": "As a Bonus Action, the Dragonborn sprouts spectral wings on their back that last for 10 minutes or until they retract the wings (no action required) or have the Incapacitated condition. During that time, they have a Fly Speed equal to their Speed. Their wings appear to be made of the same energy as their Breath Weapon. Once they use this trait, they can’t use it again until they finish a Long Rest."
    }
  },

